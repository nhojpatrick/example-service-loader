	<!--scm / -->
	<scm>
		<connection>scm:git:git://github.com/nhojpatrick/example-service-loader.git</connection>
		<developerConnection>scm:git:ssh://github.com:nhojpatrick/example-service-loader.git</developerConnection>
		<url>http://github.com/nhojpatrick/example-service-loader/tree/master</url>
