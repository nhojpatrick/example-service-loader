	<packaging>pom</packaging>
	<!--name / -->
	<name>Service Loader Example</name>
	<!--version / -->
	<version>1.0.0</version>
	<!--description / -->
	<description>Service loader example project</description>
	<!--url / -->
	<url>https://github.com/nhojpatrick/example-service-loader</url>
	<!--prerequisites / -->
	<!--issueManagement / -->
	<!--ciManagement / -->
	<!--inceptionYear / -->
	<inceptionYear>2018</inceptionYear>
	<!--mailingLists / -->
	<!--developers / -->
	<developers>
		<developer>
			<name>John Patrick</name>
			<email>142304+nhojpatrick@users.noreply.github.com</email>
		</developer>
